{
	"name": "drago-ex/dirs",
	"type": "library",
	"description": "Easier access to individual directories in an application.",
	"license": ["MIT"],
	"authors": [
		{
			"name": "Zdeněk Papučík",
			"homepage": "https://github.com/accgit"
		}
	],
	"require": {
		"php": ">=7.1",
		"nette/di": "^3.0",
		"nette/utils": "^3.0"
	},
	"replace": {
		"drago-fw/dirs": "self.version"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"minimum-stability": "dev",
	"extra": {
		"branch-alias": {
			"dev-master": "2.0.x-dev"
		}
	}
}
